<% include partials/header %>
    <ul>
        <% libraries.forEach( library => { %>
            <li>
                <a href="/libraries/<%= library.id %>">
                    <%= library.name %>, 
                    <%= library.location %>, 
                    <%= library.rating %> 
                </a>
            </li>
        <% }); %>
    </ul>
    
    <a href="/libraries/new">
        Add new library
    </a>
<% include partials/footer %>